Xcursor.theme: Vanilla-DMZ

! urxvt config
urxvt*termName: rxvt-256color
urxvt*buffered: true
urxvt*boldFont: -misc-fixed-bold-r-normal--14-130-75-75-c-70-iso10646-1
urxvt*font:   -misc-fixed-medium-r-normal--14-130-75-75-c-70-iso10646-1
             
!urxvt*boldFont: -misc-fixed-bold-r-normal--14---75---iso8859-1
urxvt*secondaryScroll:true
urxvt*saveLines: 6000
urxvt*scrollBar: false
urxvt*cursorBlink: true
!urxvt*visualBell: true
urxvt*urgentOnBell: true
urxvt*cursorColor: #FFCC00
urxvt*externalBorder: 0
!urxv*internalBorder: 5
!urxvt*cursorUnderline: true

! handle urls
urxvt*perl-ext-common:	default,matcher
urxvt*urlLauncher: chromium
urxvt.matcher.button: 2
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

! teh transparency stuff
urxvt*depth:32
!urxvt*inheritPixmap: true
urxvt*tintColor: #434343
!urxvt*shading: 40
!urxvt*transparent: true

! change default colors
!urxvt*background: #010101
urxvt*background:rgba:0021/0021/0021/cccc
urxvt*foreground: #A8A8A8
urxvt*color0: #212121
urxvt*color1: #A80000
urxvt*color2: #00A800
urxvt*color3: #A85400
urxvt*color4: #0000A8
urxvt*color5: #A800A8
urxvt*color6: #00A8A8
urxvt*color7: #A8A8A8
urxvt*color8: #545054
urxvt*color9: #F85450
urxvt*color10: #50FC50
urxvt*color11: #F2FC50
urxvt*color12: #5054F8
urxvt*color13: #F854F8
urxvt*color14: #50FCF8
urxvt*color15: #F8FCF8

